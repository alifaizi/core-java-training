package com.java.project1.package1;

public class ClassA {
	
	// Variables: Containers that hold data temporarily 
	
	public int age = 20;
	
	// Access modifier is public - means it can be accessed from anywhere
	
	public static void main(String[] args) {
		
		ClassA obj = new ClassA();
		
		System.out.println(obj.age);
	}

}
